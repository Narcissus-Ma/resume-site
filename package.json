    "google-translate-api": "^2.3.0",
    "i18next": "^25.7.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.5.0",
    "react-modal": "^3.16.1"
  },
  "devDependencies": {
    "@ant-design/icons": "^6.0.0",
    "@types/node": "^24.3.0",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/react-modal": "^3.16.3",
    "@types/react-router-dom": "^5.3.3",
